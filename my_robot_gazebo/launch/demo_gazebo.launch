<launch>
  <arg name="model" default="$(find my_robot_gazebo)/urdf/description.xacro"/>
  <arg name="rvizconfig" default="$(find my_robot_gazebo)/rviz/urdf.rviz" />
    
  <include file="$(find my_robot_gazebo)/launch/gazebo.launch">
    <arg name="model" value="$(arg model)" />
  </include>
 
 <!--node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" /-->
 
  <include file="$(find my_robot_control)/launch/myrobot_control.launch">
    <arg name="model" value="$(arg model)" />
  </include>
 
</launch>
